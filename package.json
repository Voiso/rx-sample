{
  "name": "rx-sample",
  "author": "VOISO frontend team",
  "version": "1.1.1",
  "private": true,
  "type": "module",
  "exports": "./src/index.ts",
  "description": "rx-sample simplifies managing RxJS Observables with Effector events, ensuring streamlined subscription control in reactive applications.",
  "keywords": [
    "effector",
    "rxjs",
    "typescript",
    "observable",
    "reactive-programming",
    "subscription",
    "reactivity"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Voiso/rx-sample"
  },
  "bugs": "https://github.com/Voiso/rx-sample/issues",
  "scripts": {
    "build": "smartbundle",
    "test": "vitest",
    "lint:ts": "tsc -p tsconfig.json --noEmit",
    "pcg:publish": "tsc && vitest run && smartbundle && cd dist && npm publish --access=public"
  },
  "peerDependencies": {
    "effector": ">=23",
    "rxjs": ">=7.8.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "effector": "^23.2.2",
    "rxjs": "^7.8.1",
    "smartbundle": "^0.11.1",
    "typescript": "^5.5.3",
    "vitest": "^2.0.5"
  },
  "engines": {
    "pnpm": "^9.0.0"
  }
}
